{
  "manifest_version": 3,
  "name": "ballad",
  "version": "1.0",
  "permissions": ["identity", "tabs", "storage"],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [{
    "matches": ["*://balatrodle.com/*", "*://www.balatrodle.com/*"],
    "js": ["dist/content.js"]
  }],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}